<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sas="http://www.swiss-as.com/XML" attributeFormDefault="unqualified" elementFormDefault="unqualified" version="2.02">
    <!--===ROOT===TRANSFER_WORKORDER===-->
    <xs:element name="transferWorkorder">
        <xs:annotation>
            <xs:documentation>Root element for WORKORDER form.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="workorder" type="workorderType">
                    <xs:annotation>
                        <xs:documentation>Workorders to be exported/imported.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute fixed="2.6" name="version" use="required">
                <xs:annotation>
                    <xs:documentation>Version of the schema.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!--===AMOS REFERENCE TYPES===-->
    <xs:complexType name="amosReferenceExportStructuralDamage">
        <xs:simpleContent>
            <xs:extension base="positiveInteger">
                <xs:attribute fixed="exportStructuralDamage" name="base" use="required">
                    <xs:annotation>
                        <xs:documentation>Indication of the AIM service used to produce the XML file containing the structural damage information.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="referenceDocument" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation>Reference of the XML file containing the structural damage information.</xs:documentation>
                        <xs:appinfo>AMOS_NO_DB</xs:appinfo>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <!--===COMMON COMPLEX TYPES===-->
    <!-- DownloadableLink -->
    <xs:complexType name="downloadableLinkType">
        <xs:sequence>
            <xs:element name="url" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Complete download URL for this linked document.</xs:documentation>
                    <xs:appinfo>AMOS_NO_DB_EXPORT</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="mimeType" type="xs:string">
                <xs:annotation>
                    <xs:documentation>The linked document content's mime type (e.g. "application/octet-stream").</xs:documentation>
                    <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                    <xs:appinfo>AMOS_NO_DB_EXPORT</xs:appinfo>
                    <xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="sizeKb" type="xs:long">
                <xs:annotation>
                    <xs:documentation>The linked document's size in kilobytes.</xs:documentation>
                    <xs:appinfo>AMOS_NO_DB_EXPORT</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_INTEGER</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="filename">
                <xs:annotation>
                    <xs:documentation>The linked document's name.</xs:documentation>
                    <xs:appinfo>AMOS_DATABASE_EXPORT=ste_metadata.meta_value</xs:appinfo>
                    <xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="120"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="lastModified" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>The date and time when the linked document was modified.</xs:documentation>
                    <xs:appinfo>AMOS_NO_DB_EXPORT</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- ReferenceType -->
    <xs:complexType name="referenceType">
        <xs:sequence>
            <xs:element minOccurs="0" name="category">
                <xs:annotation>
                    <xs:documentation>AMOS link ref type.</xs:documentation>
                    <xs:appinfo>AMOS_DATABASE=db_link.ref_type</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="type">
                <xs:annotation>
                    <xs:documentation>AMOS link type.</xs:documentation>
                    <xs:appinfo>AMOS_DATABASE_EXPORT=db_link.destination_type</xs:appinfo>
                    <xs:appinfo>AMOS_DATABASE_IMPORT=db_link.ref_type</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                    <xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="reference">
                <xs:annotation>
                    <xs:documentation>Referenced document identifier.</xs:documentation>
                    <xs:appinfo>AMOS_DATABASE_EXPORT=db_link.destination_key</xs:appinfo>
                    <xs:appinfo>AMOS_DATABASE_IMPORT=db_link.description</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                    <xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT=255</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="600"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="typeDescription">
                <xs:annotation>
                    <xs:documentation>Dynamic description of the link type.</xs:documentation>
                    <xs:appinfo>AMOS_NO_DB_EXPORT</xs:appinfo>
                    <xs:appinfo>AMOS_DATABASE_IMPORT=db_link.description</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="32"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="description">
                <xs:annotation>
                    <xs:documentation>AMOS link description.</xs:documentation>
                    <xs:appinfo>AMOS_DATABASE=db_link.description</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="remarks">
                <xs:annotation>
                    <xs:documentation>Remarks about the reference.</xs:documentation>
                    <xs:appinfo>AMOS_DATABASE=db_link.link_remarks</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="downloadableLink" type="downloadableLinkType">
                <xs:annotation>
                    <xs:documentation>Link information to download the file related with the reference/link.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute ref="uuid" use="optional">
            <xs:annotation>
                <xs:documentation>UUID of the reference.</xs:documentation>
                <xs:appinfo>AMOS_DATABASE=db_link.uuid</xs:appinfo>
                <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <!-- WORKORDER Type-->
    <xs:complexType name="workorderType">
        <xs:sequence>
            <xs:element minOccurs="0" name="workorderNumber" type="positiveInteger">
                <xs:annotation>
                    <xs:documentation>Workorder number. On import in AMOS, this number is ignored unless the appropriate option is activated in the interface configuration.</xs:documentation>
                    <xs:appinfo>AMOS_DATABASE=wo_header.event_perfno_i</xs:appinfo>
                    <xs:appinfo>AMOS_MIMETYPE=D_INTEGER</xs:appinfo>
                    <xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="workorderHeader">
                <xs:annotation>
                    <xs:documentation>Header information of the workorder.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="workorderType">
                            <xs:annotation>
                                <xs:documentation>M=Maintenance; P=Pirep; S= Scheduled; C=Cabin</xs:documentation>
                                <xs:appinfo>AMOS_DATABASE=wo_header.type</xs:appinfo>
                                <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                                <xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:maxLength value="8"/>
                                    <xs:minLength value="1"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="workorderState">
                            <xs:annotation>
                                <xs:documentation>State of the workorder: O = open, C = closed</xs:documentation>
                                <xs:appinfo>AMOS_DATABASE=wo_header.state</xs:appinfo>
                                <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:maxLength value="2"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="workorderOriginType">
                            <xs:annotation>
                                <xs:documentation>Workorder origin type (mandatory for Scheduled WOs): 
- Documents: AD, SB, EO,....
- Rotables: "R"
- Checks: "C" for all mandatory Checks</xs:documentation>
                                <xs:appinfo>AMOS_DATABASE=wo_header_4.event_type_mandatory</xs:appinfo>
                                <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:maxLength value="6"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:choice>
                            <xs:element name="aircraft">
                                <xs:annotation>
                                    <xs:documentation>Aircraft related to the workorder.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element minOccurs="0" name="aircraftType">
                                            <xs:annotation>
                                                <xs:documentation>Aircraft type.</xs:documentation>
                                                <xs:appinfo>AMOS_DATABASE=aircraft.ac_typ</xs:appinfo>
                                                <xs:appinfo>AMOS_MIMETYPE=E_AC_TYPE</xs:appinfo>
                                            </xs:annotation>
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:minLength value="2"/>
                                                    <xs:maxLength value="6"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="aircraftRegistration" type="acSimpleType">
                                            <xs:annotation>
                                                <xs:documentation>Registration code of the aircraft.</xs:documentation>
                                                <xs:appinfo>AMOS_DATABASE=wo_header.ac_registr</xs:appinfo>
                                                <xs:appinfo>AMOS_MIMETYPE=E_AC</xs:appinfo>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element minOccurs="0" name="workorderScheduledSubType">
                                            <xs:annotation>
                                                <xs:documentation>Scheduled workorder sub-types.
ONLY HANDLED IF workorderType = S</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element minOccurs="0" name="feedbackRequired" type="yesNoType">
                                                        <xs:annotation>
                                                            <xs:documentation>Is a feedback required ?
Only filled if is true.</xs:documentation>
                                                            <xs:appinfo>AMOS_DATABASE=wo_header_crx.feedback_req</xs:appinfo>
                                                            <xs:appinfo>AMOS_MIMETYPE=D_BOOLEAN</xs:appinfo>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element minOccurs="0" name="briefingCard" type="yesNoType">
                                                        <xs:annotation>
                                                            <xs:documentation>Is there a briefing card required ?
Only filled if is true.</xs:documentation>
                                                            <xs:appinfo>AMOS_DATABASE=wo_header_crx.briefing_card</xs:appinfo>
                                                            <xs:appinfo>AMOS_MIMETYPE=D_BOOLEAN</xs:appinfo>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element minOccurs="0" name="removedInServiceableCondition" type="yesNoType">
                                                        <xs:annotation>
                                                            <xs:documentation>Is there a part removed is serviceable condition ?
Only filled if is true.</xs:documentation>
                                                            <xs:appinfo>AMOS_DATABASE=wo_header.cannibalized</xs:appinfo>
                                                            <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element minOccurs="0" name="routineChecks">
                                            <xs:annotation>
                                                <xs:documentation>Routine checks list.
ONLY HANDLED IF workorderType = S</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element maxOccurs="unbounded" name="routineCheck">
                                                        <xs:annotation>
                                                            <xs:documentation>Routine check, e.g. Daily Check.</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="checkCode">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Routine check code:
DC = Daily Check
WC = Weekly Check
BWC = Bi-Weekly Check
PFC = Pre-Flight Check
MPC = Maintenance Pre-Flight Check
SOC = Stay Over Check
TC = Transit Check</xs:documentation>
                                                                        <xs:appinfo>AMOS_NO_DB</xs:appinfo>
                                                                    </xs:annotation>
                                                                    <xs:simpleType>
                                                                        <xs:restriction base="xs:string">
                                                                            <xs:maxLength value="4"/>
                                                                            <xs:minLength value="1"/>
                                                                        </xs:restriction>
                                                                    </xs:simpleType>
                                                                </xs:element>
                                                                <xs:element name="performed" type="yesNoType">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Check performed ?
Y=Yes
N=No</xs:documentation>
                                                                        <xs:appinfo>AMOS_NO_DB</xs:appinfo>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="component">
                                <xs:annotation>
                                    <xs:documentation>Component related to the workorder.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="partNumber" type="partNumberType">
                                            <xs:annotation>
                                                <xs:documentation>Part number of the component.</xs:documentation>
                                                <xs:appinfo>AMOS_DATABASE=wo_header.comp_partno</xs:appinfo>
                                                <xs:appinfo>AMOS_MIMETYPE=M_PART</xs:appinfo>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="serialNumber" type="serialNumberType">
                                            <xs:annotation>
                                                <xs:documentation>Serial number of the component.</xs:documentation>
                                                <xs:appinfo>AMOS_DATABASE=wo_header.comp_serialno</xs:appinfo>
                                                <xs:appinfo>AMOS_MIMETYPE=M_SERIAL_NUMBER</xs:appinfo>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element minOccurs="0" name="removedInServiceableCondition" type="yesNoType">
                                            <xs:annotation>
                                                <xs:documentation>Is there a part removed is serviceable condition ? Only filled if is true.
ONLY HANDLED IF workorderType = S</xs:documentation>
                                                <xs:appinfo>AMOS_DATABASE=wo_header.cannibalized</xs:appinfo>
                                                <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:choice>
                        <xs:element minOccurs="0" name="workpackageName" type="workpackageNumberComplexType">
                            <xs:annotation>
                                <xs:documentation>Name of the workpackage if workorder is linked to one of them.</xs:documentation>
                                <xs:appinfo>AMOS_DATABASE=wp_header.wpno</xs:appinfo>
                                <xs:appinfo>AMOS_MIMETYPE=E_WORKPACKAGE_AC</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="projectNumber">
                            <xs:annotation>
                                <xs:documentation>Project number linked to the workorder.</xs:documentation>
                                <xs:appinfo>AMOS_DATABASE=wo_header.projectno</xs:appinfo>
                                <xs:appinfo>AMOS_MIMETYPE=F_PROJECT_STR</xs:appinfo>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:minLength value="1"/>
                                    <xs:maxLength value="14"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="barcode">
                            <xs:annotation>
                                <xs:documentation>Barcode identifying the workorder to use on printed documentation.</xs:documentation>
                                <xs:appinfo>AMOS_NO_DB</xs:appinfo>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:minLength value="1"/>
                                    <xs:maxLength value="16"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="externalWorkorderNumber">
                            <xs:annotation>
                                <xs:documentation>External Workorder number, used to have a reference to a workorder number from another system.</xs:documentation>
                                <xs:appinfo>AMOS_DATABASE=wo_header.ext_workorderno</xs:appinfo>
                                <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                                <xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:maxLength value="36"/>
                                    <xs:minLength value="1"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="ataChapter">
                            <xs:annotation>
                                <xs:documentation>ATA chapter.</xs:documentation>
                                <xs:appinfo>AMOS_DATABASE=wo_header.ata_chapter</xs:appinfo>
                                <xs:appinfo>AMOS_MIMETYPE=G_ATA_CHAPTER_STR</xs:appinfo>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:maxLength value="12"/>
                                    <xs:minLength value="1"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="issueData" type="issueType">
                            <xs:annotation>
                                <xs:documentation>Issue information for the workorders.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="classifications">
                            <xs:annotation>
                                <xs:documentation>Workorder classifications information.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element minOccurs="0" name="administrativeServiceCheck" type="yesNoType">
                                        <xs:annotation>
                                            <xs:documentation>Administrative service check</xs:documentation>
                                            <xs:appinfo>AMOS_DATABASE=wo_header_more.admin_wo_only</xs:appinfo>
                                            <xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element minOccurs="0" name="aogRisk" type="yesNoType">
                                        <xs:annotation>
                                            <xs:documentation>AOG risk</xs:documentation>
