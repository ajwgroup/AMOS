<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sas="http://www.swiss-as.com/XML" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="0.1">
	<xs:element name="transferPartDefinition">
		<xs:annotation>
			<xs:documentation>Root element for transferPartDefinition form.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element name="part" type="partType">
					<xs:annotation>
						<xs:documentation>Part to be exported/inserted/updated.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="version" use="required" fixed="1.1"/>
		</xs:complexType>
	</xs:element>
	<!-- COMMON TYPES -->
	<!-- SIMPLE ISO8601 DATE -->
	<xs:simpleType name="simpleDateType">
		<xs:annotation>
			<xs:documentation>Date type. ISO8601-date as string or empty string.</xs:documentation>
			<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([12][0-9][0-9][0-9]\-((0[1-9])|(1[0-2]))\-((0[1-9])|([12][0-9])|([3][01])))?"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- YES OR NO -->
	<xs:simpleType name="yesNoType">
		<xs:annotation>
			<xs:documentation>Y=YES; N=NO.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>YES.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>NO.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- PRIMITIVE TYPES -->
	<xs:simpleType name="positiveInteger">
		<xs:annotation>
			<xs:documentation>Zero or positive integer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="2147483647"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="double">
		<xs:annotation>
			<xs:documentation>Limited to 17 digits overall</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="17"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="positiveDouble">
		<xs:annotation>
			<xs:documentation>Zero or positive decimal number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="17"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ATA CHAPTER -->
	<xs:simpleType name="ataChapter">
		<xs:restriction base="xs:string">
			<xs:maxLength value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- STATION -->
	<xs:simpleType name="station">
		<xs:annotation>
			<xs:documentation>Max. 4 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- STORE -->
	<xs:simpleType name="store">
		<xs:annotation>
			<xs:documentation>Max. 8 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- LOCATION -->
	<xs:simpleType name="location">
		<xs:annotation>
			<xs:documentation>Max. 16 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="16"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- DESCRIPTION OR REMARK -->
	<xs:simpleType name="description">
		<xs:annotation>
			<xs:documentation>A description or remark up to 36 characters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="36"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- AC_TYPE -->
	<xs:simpleType name="acSimpleType">
		<xs:annotation>
			<xs:documentation>Use for ac register number or ac type. Max. 6 characters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- USER SIGN -->
	<xs:simpleType name="userSignSimpleType">
		<xs:annotation>
			<xs:documentation>Any user sign. Max. 8 characters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- PARTNUMBER DEFINITION -->
	<xs:simpleType name="partNumberType">
		<xs:annotation>
			<xs:documentation>Type for partnumbers: minLentgh=1; maxLength=32;</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<!--PARTSERIALNUMBER DEFINITION -->
	<xs:simpleType name="partSerialNumberType">
		<xs:annotation>
			<xs:documentation>Type for part-serialnumbers: minLength=1; maxLength=20</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- COUNTER -->
	<xs:simpleType name="counterCodeType">
		<xs:annotation>
			<xs:documentation>D=Days; H=Hours; C=Cycles; or Custom type. Must be up to 10 characters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- MATERIAL CLASS DEFINITION -->
	<xs:simpleType name="materialClassType">
		<xs:annotation>
			<xs:documentation>Type for material classes. R=Rotable; C=Consumable; S=Service; E=Expandable; or custom type (see basic table Material Class). Max. 4 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- PRICE TYPE DEFINITION -->
	<xs:complexType name="priceType">
		<xs:annotation>
			<xs:documentation>Pricing information.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="double">
				<xs:attribute name="internationalCurrencyCode" use="required">
					<xs:annotation>
						<xs:appinfo>
							<xs:appinfo>AMOS_DATABASE=part_custom.currency</xs:appinfo>
							<xs:appinfo>AMOS_DATABASE=entity_header.currency</xs:appinfo>
							<xs:appinfo>AMOS_MIMETYPE=D_CURRENCY</xs:appinfo>
							<xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT</xs:appinfo>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="3"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!-- COMMON TYPES finished-->
	<xs:group name="acPos">
		<xs:sequence>
			<xs:element name="ac" type="acSimpleType">
				<xs:annotation>
					<xs:appinfo>AMOS_MIMETYPE=E_AC</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=rotables.ac_registr</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=E_AC</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pos" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>AMOS_DATABASE=rotables.location</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="stationStoreLocation">
		<xs:sequence>
			<xs:element name="station" type="station">
				<xs:annotation>
					<xs:appinfo>AMOS_MIMETYPE=A_STATION</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=location.station</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="store" type="store" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>AMOS_DATABASE=location.store</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_STORE</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="location" type="location" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>AMOS_DATABASE=location.location</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=M_LOCATION</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<!-- PART -->
	<xs:complexType name="partType">
		<xs:all>
			<xs:element name="partNumber" type="partNumberType">
				<xs:annotation>
					<xs:documentation>Part number according to ATA.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=M_PART</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=part.partno</xs:appinfo>
					<xs:documentation>On IMPORT it is always changed to upper case.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="partHeader" type="partHeaderType">
				<xs:annotation>
					<xs:documentation>Basic information about the part.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aircraftTypeList" type="aircraftTypeList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of aircraft-types the part can be installed in.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="positionList" type="positionList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of positions on an aircraft at which the part can be installed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vendorList" type="vendorList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of vendors of the part.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternateList" type="alternateList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of alternates of the part.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="classificationList" type="classificationList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of classifications for the part. (e.g. ENGINE, APU,DANGEROUS GOODS...)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="remarks" type="remarks" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of remarks to the part.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specialCounters" type="counterList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of non standard counters (different than H, C and D) defined for the part.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="entityInformation" type="entityInformationType" minOccurs="0"/>
			<xs:element name="pricing" type="pricing" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pricing information for the part.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="locationList" type="locationList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of locations where this part is stored.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customsList" type="customsList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of customs for the part.</xs:documentation>
					<xs:documentation> EXPORT ONLY.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!-- REMARKS -->
	<xs:complexType name="remarks">
		<xs:all minOccurs="0">
			<xs:element name="purchaseRemarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Remarks for purchasing.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_remarks.text</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="internalRemarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Internal remarks. (internal use.)</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_description.text</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="repairRemarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Remarks for repair.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_remarks.text</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="receivingRemarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Remarks for receiving.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_remarks.text</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pickslipRemarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Remarks for pickslips.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_remarks.text</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultRemarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>AMOS_DATABASE=part_remarks.text</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="partRemarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>These remarks show up on main tabsheet in Part Administration.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part.remarks</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!--COUNTER_LIST -->
	<xs:complexType name="counterList">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="specialCounterCode" type="counterCodeType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Counter's code.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=counter_definition.code</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--commodity code list -->
	<xs:complexType name="customsList">
		<xs:sequence>
			<xs:element name="customs" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="customsCode">
							<xs:annotation>
								<xs:appinfo>AMOS_DATABASE_EXPORT=address.vendor</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=A_VENDOR</xs:appinfo>
								<xs:documentation>The code of the customs.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="12"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="commodityCode">
							<xs:annotation>
								<xs:appinfo>AMOS_DATABASE_EXPORT=part_custom.commodity_code</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
								<xs:documentation>Commodity code for the customs.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- PRICING -->
	<xs:complexType name="pricing">
		<xs:all minOccurs="0">
			<xs:element name="averagePrice" type="priceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Local average price of the part. Setting the Average Price is not allowed with FSV based Moving Average Price (when parameter 1532 is set to “Financial Stock Value Based Moving Average Price”).</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_fa_entity.average_price</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_MON</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="functionalAveragePrice" type="priceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Functional average price of the part.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_fa_entity.average_price_func</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_MON</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="valueForCustoms" type="priceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customs value of the part.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_custom.value</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_MON</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="avgRepairCost" type="priceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Local average costs for repair.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_fa_entity.avg_repair_cost</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_MON</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="functionalAvgRepairCost" type="priceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Functional average costs for repair.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_fa_entity.avg_repair_cost_func</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_MON</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchasePrice" type="priceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Current price for purchasing. (Last order.)</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_fa_entity.purch_price</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_MON</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesPrice" type="priceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Price when selling the part.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_fa_entity.sales_price</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_MON</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!-- VENDOR_LIST -->
	<xs:complexType name="vendorList">
		<xs:sequence minOccurs="0">
			<xs:element name="vendor" type="vendorType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Vendor the part can be bought at.</xs:documentation>
				</xs:annotation>
			</xs:element>
