<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sas="http://www.swiss-as.com/XML" attributeFormDefault="unqualified" elementFormDefault="unqualified" version="0.1">
    <!-- TRANSFER_Order -->
    <xs:element name="transferOrder">
        <xs:annotation>
            <xs:documentation>Root element for ORDER form.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="order" type="OrderType">
                    <xs:annotation>
                        <xs:documentation>Order to be uploaded/changed/deleted.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute fixed="2.1" name="version" use="required">
                <xs:annotation>
                    <xs:documentation>Version of the schema.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- SIMPLE TYPES -->
    <!-- PRIMITIVE TYPES -->
    <xs:simpleType name="PositiveIntegerType">
        <xs:annotation>
            <xs:documentation>Zero or positive integer</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="2147483647"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IntegerType">
        <xs:restriction base="xs:int">
            <xs:minInclusive value="-2147483648"/>
            <xs:maxInclusive value="2147483647"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DoubleType">
        <xs:annotation>
            <xs:documentation>Limited to 17 digits overall</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="17"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PositiveDoubleType">
        <xs:annotation>
            <xs:documentation>Zero or positive decimal number</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="17"/>
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- END OF PRIMITIVE TYPES -->
    <xs:simpleType name="SimpleDateType">
        <xs:annotation>
            <xs:documentation>Date type. ISO8601-date as string or empty string.</xs:documentation>
            <xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="([1-9][0-9][0-9][0-9]\-((0[1-9])|(1[0-2]))\-((0[1-9])|([12][0-9])|([3][01])))?"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SimpleTimeUtcType">
        <xs:annotation>
            <xs:documentation>Time type. UTC-time as string or empty string.</xs:documentation>
            <xs:appinfo>AMOS_MIMETYPE=D_TIME</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="(([01][0-9][:][0-5][0-9])|(2[0-3][:][0-5][0-9]))?"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="YesNoType">
        <xs:annotation>
            <xs:documentation>Y=YES; N=NO.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Y">
                <xs:annotation>
                    <xs:documentation>YES.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="N">
                <xs:annotation>
                    <xs:documentation>NO.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AcSimpleType">
        <xs:annotation>
            <xs:documentation>Use for ac register number or ac type. Max. 6 characters.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PartNumberType">
        <xs:annotation>
            <xs:documentation>Type for partnumbers: minLength=1; maxLength=20;</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="32"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PartSerialNumberType">
        <xs:annotation>
            <xs:documentation>Type for part-serialnumbers: minLength=1; maxLength=20</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PartBatchNumberType">
        <xs:annotation>
            <xs:documentation>Type for part-batchnumbers: minLength=1; maxLength=20</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- END OF SIMPLE TYPES -->
    <xs:complexType name="PriceType">
        <xs:annotation>
            <xs:documentation>Pricing information.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="DoubleType">
                <xs:attribute name="internationalCurrencyCode" use="required">
                    <xs:annotation>
                        <xs:documentation>Price currency.</xs:documentation>
                        <xs:appinfo>AMOS_DATABASE_EXPORT=address.currency</xs:appinfo>
                        <xs:appinfo>AMOS_MIMETYPE=D_CURRENCY</xs:appinfo>
                        <xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT=4</xs:appinfo>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                            <xs:maxLength value="3"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="amosReferenceTransferPart">
        <xs:simpleContent>
            <xs:extension base="PartNumberType">
                <xs:attribute fixed="transferPartDefinition" name="base" use="required">
                    <xs:annotation>
                        <xs:documentation>Fixed value: "transferPartdefinition".</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="referenceDocument" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation>Reference document.</xs:documentation>
                        <xs:appinfo>AMOS_NO_DB_EXPORT</xs:appinfo>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
