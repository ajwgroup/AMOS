<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sas="http://www.swiss-as.com/XML" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="0.41">
	<!-- transferShipment -->
	<xs:element name="transferShipment">
		<xs:annotation>
			<xs:documentation>This layout can be used to import / export shipment data to / from AMOS.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element name="shipment">
					<xs:annotation>
						<xs:documentation>Shipment to be created or updated.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="shipmentType">
								<xs:attribute ref="uuid">
									<xs:annotation>
										<xs:documentation>UUID information</xs:documentation>
										<xs:appinfo>AMOS_DATABASE=sh_header.uuid</xs:appinfo>
										<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="version" use="required" fixed="1.6">
				<xs:annotation>
					<xs:documentation>schema version</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!-- COMMON TYPES -->
	<!-- YES OR NO -->
	<xs:simpleType name="yesNoType">
		<xs:annotation>
			<xs:documentation>Y=YES; N=NO.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>YES.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>NO.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- PRIMITIVE TYPES -->
	<xs:simpleType name="positiveInteger">
		<xs:annotation>
			<xs:documentation>Zero or positive integer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="double">
		<xs:annotation>
			<xs:documentation>Limited to 17 digits overall</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="17"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="positiveDouble">
		<xs:annotation>
			<xs:documentation>Zero or positive decimal number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="17"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- MEASURE UNIT -->
	<xs:simpleType name="unitOfMeasurementType">
		<xs:annotation>
			<xs:appinfo>AMOS_DATABASE=sh_header.measure_unit</xs:appinfo>
			<xs:appinfo>AMOS_DATABASE=sh_package.measure_unit</xs:appinfo>
			<xs:appinfo>AMOS_DATABASE=sh_detail.weight_unit</xs:appinfo>
			<xs:appinfo>AMOS_MIMETYPE=M_MEASURE_UNIT</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- PARTNUMBER DEFINITION -->
	<xs:simpleType name="partNumberType">
		<xs:annotation>
			<xs:documentation>Type for partnumbers: minLength=1; maxLength=32;</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<!--PARTSERIALNUMBER DEFINITION -->
	<xs:simpleType name="partSerialNumberType">
		<xs:annotation>
			<xs:documentation>Type for part-serialnumbers: minLength=1; maxLength=20</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- amosAddressType -->
	<xs:complexType name="amosAddressType">
		<xs:all>
			<xs:element name="code">
				<xs:annotation>
					<xs:documentation>Code representing the address or user sign in AMOS.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=A_VENDOR</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_USER_SIGN</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/billingAddress)=address.vendor[sh_header.bill_to]</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/createdBy)=sh_header.created_by</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/destinationAddress)=address.vendor[sh_header.destination]</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/issuerAddress)=address.vendor[sh_header.issued_by]</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/originAddress)=address.vendor[sh_header.origin_address_i]</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/shipperAddress)=address.vendor[sh_header.shipper]</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentItems/package/packedBy)=sh_package.packed_by</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name in AMOS. EXPORT ONLY.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=address.name</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="contactName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contact name in AMOS. EXPORT ONLY.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=address.contact_name</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="address" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Address in AMOS. EXPORT ONLY.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=adr_details.address</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="city" minOccurs="0">
				<xs:annotation>
					<xs:documentation>City of address in AMOS. EXPORT ONLY.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=adr_details.city</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="zipCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ZIP-code of address in AMOS. EXPORT ONLY.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=adr_details.zip_code</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="country" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Country of address in AMOS.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=adr_details.country</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=COUNTRY</xs:appinfo>
					<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="weightInformationType">
		<xs:all>
			<xs:element name="totalWeight" type="positiveDouble" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Gross weight.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_DOUBLE</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/weightInformation)=sh_header.total_weight</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentItems/package/weightInformation)=sh_package.weight</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalVolWeight" type="positiveDouble" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Volume weight.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_DOUBLE</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/weightInformation)=sh_header.total_vol_weight</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentItems/package/weightInformation)=sh_package.vol_weight</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalNetWeight" type="positiveDouble" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Netto weight.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_DOUBLE</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/weightInformation)=sh_header.total_net_weight</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentItems/package/weightInformation)=sh_package.net_weight</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="weightUOM" type="unitOfMeasurementType">
				<xs:annotation>
					<xs:documentation>Unit of measurement for weight information.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=M_MEASURE_UNIT</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentHeader/weightInformation)=sh_header.measure_unit</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE(/transferShipment/shipment/shipmentItems/package/weightInformation)=sh_package.measure_unit</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!-- COMMON TYPES finished -->
	<!-- shipmentType -->
	<xs:complexType name="shipmentType">
		<xs:sequence>
			<xs:element name="shipmentHeader" type="shipmentHeaderType">
				<xs:annotation>
					<xs:documentation>The shipment header.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shipmentItems" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shipment items: details and packages.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence maxOccurs="unbounded">
						<xs:choice>
							<xs:element name="package" type="shipmentPackageType">
								<xs:annotation>
									<xs:documentation>Shipment package data.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="detail" type="shipmentDetailType">
								<xs:annotation>
									<xs:documentation>Shipment detail data.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- shipmentHeaderType-->
	<xs:complexType name="shipmentHeaderType">
		<xs:all>
			<xs:element name="shipmentNumber" type="positiveInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The shipment unique identification number.</xs:documentation>
					<xs:documentation>EXPORT ONLY</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=sh_header.shipmentno_i</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=M_SHIPMENT3</xs:appinfo>
					<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="awbNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The document's air waybill number.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE_EXPORT=sh_header.awbno</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE_IMPORT=sh_header.awbno</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="24"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="shipmentType">
				<xs:annotation>
					<xs:documentation>Shipment type: INCOMING_SHIPMENT = EI; OUTGOING_SHIPMENT = EO;</xs:documentation>
					<xs:documentation>EXPORT or INSERT, no UPDATE</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.type</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="EI"/>
						<xs:enumeration value="EO"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="awbDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when the air waybill  was given.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.awbdate</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="internalShipment" type="yesNoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Is the shipment an internal shipment - Y or N.</xs:documentation>
					<xs:documentation>EXPORT or INSERT, no UPDATE.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.is_internal</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_BOOLEAN</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalShipmentNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If shipment was created from another shipment, the other shipment`s number will be saved here.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.ext_shipmentno</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="estimatedArrival" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The estimated arrival date or dateTime of the shipment.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="date" type="xs:date">
							<xs:annotation>
								<xs:documentation>The estimated arrival date of the shipment.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_header.arrival_date</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="dateTime" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>The estimated arrival date and time of the shipment.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_header.arrival_date</xs:appinfo>
								<xs:appinfo>AMOS_DATABASE=sh_header.arrival_time</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_DATE_TIME</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="accountNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Account number for the shipper. Defined in Address Management.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.accountno</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="aaNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Holds information about the AA-No. (Ausfuhranmeldung)</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.aa_no</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="shipmentStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shipment status: SENT = S, ARRIVED = A, RECEIVED = R, RTLY_RECEIVED = X, PARTLY_ARRIVED = P,   TRANSFER = T, OPEN = O,  CLOSED = C, READY = RP;</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.state</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="S"/>
						<xs:enumeration value="A"/>
						<xs:enumeration value="T"/>
						<xs:enumeration value="X"/>
						<xs:enumeration value="P"/>
						<xs:enumeration value="O"/>
						<xs:enumeration value="C"/>
						<xs:enumeration value="R"/>
						<xs:enumeration value="RP"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="transportMethod" minOccurs="0">
				<xs:annotation>
					<xs:documentation>How will the shipment be transported.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="code">
							<xs:annotation>
								<xs:documentation>Method of transport code.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_header.method_of_transport</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="description" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Description of the transport method.</xs:documentation>
								<xs:documentation>EXPORT ONLY</xs:documentation>
								<xs:appinfo>AMOS_DATABASE_EXPORT=sh_transport_method.description</xs:appinfo>
								<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="flightInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If shipment is coming via plane, this element contains flight information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="legNumber" minOccurs="0">
							<xs:annotation>
								<xs:documentation>If shipment is coming via plane, the flight number can be entered here.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_header.leg_number</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="8"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="flightDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>If shipment is coming via plane, the date can be entered here.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_header.flight_date</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="flightArrival" minOccurs="0">
							<xs:annotation>
								<xs:documentation>If shipment is coming via plane, the arrival airport can be entered here.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_header.flight_arrival</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=A_STATION</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="flightDeparture" minOccurs="0">
							<xs:annotation>
								<xs:documentation>If shipment is coming via plane, the departure airport can be entered here.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_header.flight_departure</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=A_STATION</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="weightInformation" type="weightInformationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about the total weight of the whole shipment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shipmentText" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Text which will be printed on the shipment papers.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.shipment_text</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="remarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Internal remarks for the shipment.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.remarks</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="shipperAddress" type="amosAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Who will deliver the shipment.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.shipper</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_VENDOR</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="billingAddress" type="amosAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Who is going to pay for the shipping.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.bill_to</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_VENDOR</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="destinationAddress" type="amosAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Destination of the shipment.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.destination</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_VENDOR</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="originAddress" type="amosAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Origin of the shipment.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.origin_address_i</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_VENDOR</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="issuerAddress" type="amosAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Who issued this shipment. This data is mandatory when creating a new shipment. This data is optional when updating an existing shipment. Only address code existing in AMOS will be applied on the shipment. In case of using address code that doesn't exist in AMOS, an error is reported for insert and a warning for an update.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.issued_by</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_VENDOR</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date/time the shipment has been delivered on.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.delivery_date</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=sh_header.delivery_time</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_DATE_TIME</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="createdBy" type="amosAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Person who created the shipment.</xs:documentation>
					<xs:documentation>EXPORT and INSERT, no UPDATE.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.created_by</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_USER_SIGN</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="typeOfService" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of service code.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=sh_type_of_service.code[sh_type_of_service.type_of_serviceno_i=sh_header.type_of_serviceno_i]</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="numberOfPackages" type="positiveInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number of packages included in this shipment. (Zero or more)</xs:documentation>
					<xs:documentation>EXPORT ONLY</xs:documentation>
					<xs:appinfo>AMOS_NO_DB_EXPORT</xs:appinfo>
					<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_INTEGER</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="currentLocation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Current location of the shipment. This field is mandatory when creating a new shipment.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="station">
							<xs:annotation>
								<xs:documentation>Station of the current location of the shipment.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=location.station[location.locationno_i=sh_header.locationno_i]</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=A_STATION</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="store">
							<xs:annotation>
								<xs:documentation>Store of the current location of the shipment.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=location.store[location.locationno_i=sh_header.locationno_i]</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=A_STORE</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="8"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="location">
							<xs:annotation>
								<xs:documentation>Location code of the current location of the shipment.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=location.location[location.locationno_i=sh_header.locationno_i]</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=M_LOCATION</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="16"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="incoterm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Incoterm.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_header.incoterm</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="customsCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The code of the customs.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=address.vendor[sh_header.customs_i]</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_VENDOR</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!-- shipmentDetailType -->
	<xs:complexType name="shipmentDetailType">
		<xs:all>
			<xs:element name="detailNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Internal number of a detail.</xs:documentation>
					<xs:documentation>EXPORT ONLY</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=sh_detail.shipdetailno_i</xs:appinfo>
					<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_INTEGER</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="quantity">
				<xs:annotation>
					<xs:documentation>Quantity of the shipped item.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.qty</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_QTY</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="positiveDouble">
							<xs:attribute name="unitOfMeasurement">
								<xs:annotation>
									<xs:documentation>Measurement unit of the part. For import must be the same as the one assigned to the part.</xs:documentation>
									<xs:appinfo>AMOS_DATABASE=part.measure_unit</xs:appinfo>
									<xs:appinfo>AMOS_MIMETYPE=M_MEASURE_UNIT</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="partInformation">
				<xs:annotation>
					<xs:documentation>Information on the part being shipped.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="partNumber" type="partNumberType">
							<xs:annotation>
								<xs:documentation>Part number of the shipped item.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_detail.partno</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=M_PART</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="serialNumber" type="partSerialNumberType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Serial number of the shipped item.</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_detail.serialno</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=M_SERIAL_NUMBER</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="partDescription" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The part's short description.</xs:documentation>
								<xs:documentation>EXPOR ONLY</xs:documentation>
								<xs:appinfo>AMOS_DATABASE_EXPORT=part.description</xs:appinfo>
								<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="36"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="labelNumber" type="positiveInteger" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Label number of the shipped item. It is set automatically when creating a shipment from an order.</xs:documentation>
								<xs:documentation>EXPORT ONLY</xs:documentation>
								<xs:appinfo>AMOS_DATABASE_EXPORT=sh_detail.labelno</xs:appinfo>
								<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_INTEGER</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="dangerousGoodInformation" type="dangerousGoodInformationType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>dangerous goods information

EXPORT ONLY</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="countryOfOrigin" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Country of origin of the part</xs:documentation>
								<xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT</xs:appinfo>
								<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
								<xs:appinfo>AMOS_DATABASE=sh_detail.customs_country_of_origin</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="pickslipInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information on the Pickslip of the shipped item.</xs:documentation>
					<xs:documentation>EXPORT ONLY</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="pickslipItemID" type="positiveInteger">
							<xs:annotation>
								<xs:documentation>Unique ID of the pickslip item.</xs:documentation>
								<xs:documentation>EXPOR ONLY</xs:documentation>
								<xs:appinfo>AMOS_DATABASE_EXPORT=pickslip_booked.pickslipseqno_i</xs:appinfo>
								<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=M_PICKSLIP_BOOKED</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="pickslipNumber" type="xs:int">
							<xs:annotation>
								<xs:documentation>Unique ID of the pickslip.</xs:documentation>
								<xs:documentation>EXPOR ONLY</xs:documentation>
								<xs:appinfo>AMOS_DATABASE_EXPORT=pickslip_detail.pickslipno</xs:appinfo>
								<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=M_PICKSLIP</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="recordNumber" type="xs:int">
							<xs:annotation>
								<xs:documentation>Item position on the pickslip. (Sort order.)</xs:documentation>
								<xs:documentation>EXPOR ONLY</xs:documentation>
								<xs:appinfo>AMOS_DATABASE_EXPORT=pickslip_detail.recno</xs:appinfo>
								<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_INTEGER</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="orderInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information on the order the shipment was made for.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="amosOrderNumber" minOccurs="0">
							<xs:annotation>
								<xs:documentation>An AMOS-order-number.</xs:documentation>
								<xs:documentation>EXPORT and INSERT, no UPDATE</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=od_header.orderno[sh_detail.od_detailno_i]</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=M_ORDER</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="12"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="orderPosition" type="positiveInteger" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The items position on an order.</xs:documentation>
								<xs:documentation>EXPORT and INSERT, no UPDATE</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=od_detail.posno[sh_detail.od_detailno_i]</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_INTEGER</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="externalOrderNumber" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>An external order number.</xs:documentation>
								<xs:documentation>EXPORT ONLY</xs:documentation>
								<xs:appinfo>AMOS_DATABASE_EXPORT=od_header.ext_orderno</xs:appinfo>
								<xs:appinfo>AMOS_NOT_IMPORTED</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="orderDetailUUID" minOccurs="0">
							<xs:annotation>
								<xs:documentation>UUID of the order detail</xs:documentation>
								<xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT</xs:appinfo>
								<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
								<xs:appinfo>AMOS_DATABASE=od_detail.uuid</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="36"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="commodityCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The detail's commodity code.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=commodity_code.commod_code</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="customsValue" type="double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shipment customs value for the detail. Given in currency stored in the "currency" field.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.price</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_DOUBLE</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="dutyValue" type="double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customs duty value. Given in the currency stored in the "currency" field.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.duty_value</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_DOUBLE</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Currency of duty value and customs value.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.currency</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_CURRENCY</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="nepForm" type="yesNoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>"Y" = NEP form required, omitted otherwise on export.</xs:documentation>
					<xs:documentation>EXPORT and INSERT, no UPDATE</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_BOOLEAN</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=sh_detail.nep_form</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="exportRegistration" type="yesNoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>"Y" if an export registration is required, omitted otherwise on export.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_BOOLEAN</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=sh_detail.export_registration</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="remarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Right now only used internally. Holds remarks for a shipment detail.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.remarks</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="remarksShort" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Short remarks of the shipment detail.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.short_remarks</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="condition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition of a detail.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.condition</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=G_CONDITION</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="unitWeight" type="positiveDouble" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Weight of a single unit of the detail. Given in the measurement unit stored in the "weightUOM" field.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.weight</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=M_MEASURE_UNIT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="weightUOM" type="unitOfMeasurementType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Measurement unit of the weight.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.weight_unit</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=M_MEASURE_UNIT</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="customsClassification" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customs classification.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=customs_classification.code[customs_classification.customs_classificationno_i=sh_detail.customs_classificationno_i]</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=M_CUSTOMS_CLASSIFICATION</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="customsno" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The number given by the customs.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.customsno</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="24"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="custClearanceDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customs clearance date.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.cust_clearance_date</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="customsEndUse" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customs end use.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_detail.customs_end_use</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="customsState" minOccurs="0">
				<xs:annotation>
					<xs:documentation>State of the shipment for the customs.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=customs_state.description</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="48"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:all>
		<xs:attribute ref="uuid">
			<xs:annotation>
				<xs:documentation>Uuid of the detail.</xs:documentation>
				<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				<xs:appinfo>AMOS_DATABASE=sh_detail.uuid</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- shipmentPackageType -->
	<xs:complexType name="shipmentPackageType">
		<xs:all>
			<xs:element name="packingNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Package number, given by the shipper.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_package.packingno</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="packedBy" type="amosAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Who packed this package.  This field is mandatory when creating a new OUTGOING shipment.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_package.packed_by</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=A_USER_SIGN</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="packedDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>When was the package packed.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_package.packed_date</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="sizeInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>size information</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="width" type="positiveDouble" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Width of the package (cm).</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_package.width</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_DOUBLE</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="length" type="positiveDouble" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Length of the package (cm).</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_package.length</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_DOUBLE</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="height" type="positiveDouble" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Height of the package (cm).</xs:documentation>
								<xs:appinfo>AMOS_DATABASE=sh_package.height</xs:appinfo>
								<xs:appinfo>AMOS_MIMETYPE=D_DOUBLE</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="weightInformation" type="weightInformationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about the weight of the package.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="packageDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of details in the package.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="detail" type="shipmentDetailType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>package detail information</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="remarks" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Package's remarks.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=sh_package.remarks</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:attribute ref="uuid">
			<xs:annotation>
				<xs:documentation>Uuid of the package</xs:documentation>
				<xs:appinfo>AMOS_DATABASE=sh_package.uuid</xs:appinfo>
				<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- UUID -->
	<xs:attribute name="uuid">
		<xs:annotation>
			<xs:documentation>UUID values</xs:documentation>
			<xs:appinfo>AMOS_CHECKED_LENGTH_IMPORT</xs:appinfo>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<!-- DGR information -->
	<xs:complexType name="dangerousGoodInformationType">
		<xs:sequence>
			<xs:element name="classDivision" minOccurs="0">
				<xs:annotation>
					<xs:documentation>class division id</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=dg_class_division.class_division</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="unNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>un number</xs:documentation>
					<xs:appinfo>AMOS_DATABASE_EXPORT=part_dangerous_goods.un_number</xs:appinfo>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="description" minOccurs="0">
				<xs:annotation>
					<xs:documentation>description information</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE_EXPORT=dg_class_division.description</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="150"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="hazardLabel" minOccurs="0">
				<xs:annotation>
					<xs:documentation>hazard label information</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE_EXPORT=dg_class_division.hazard_label</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="compatibilityGroup" minOccurs="0">
				<xs:annotation>
					<xs:documentation>compatibility group information</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_TEXT</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE_EXPORT=part_dangerous_goods.compatibility_group</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="isDGRProformaInvoice" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>information if material should be marked as DGR on Proforma Invoice</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_BOOLEAN</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE_EXPORT=sh_detail.remove_dgr_flag</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
